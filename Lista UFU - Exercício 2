# include <stdio.h>
# include <stdlib.h>
//2 - Crie um programa que calcula o comprimento de uma string (nao use a função strlen).
int main(){
	int i, contador =0;
	char string[100];
	printf("Digite a string: \n");
	fgets(string,100,stdin);
	for(i=0; string[i]!='\0';i++){
		contador++;
	}
	printf("\nO tamanho eh: \n%d", contador-1);
return 0;
}
